}

/*Navigation Style Start*/
.main-navigation {
    margin-right: -85px;
    text-align: right;
}

.main-navigation ul {
    list-style: none;
    position: relative;
    margin: 0;
    padding: 0;
    display: block;
        margin-top: 19px;
}

.main-navigation ul a {
display: block;
color: #333333;
text-decoration: none;
font-size: 20px;
padding: 15px 0 0;
text-transform: capitalize;
outline: none;
font-weight: bold;
line-height: 16px;
position: relative;
/*text-shadow: 1px 1px #ddd;*/
}

.main-navigation ul li {
    position: relative;
    margin: 0 0 0 30px;
    padding: 0;
    list-style: none;
    display: inline-block;
     vertical-align: top;
    cursor: pointer;
}

.main-navigation ul li:first-child {
    margin: 0;
}

.main-navigation ul li span {
    text-transform: uppercase;
    font-size: 10px;
}

.modern .main-navigation ul li span {
    color: #aaa;
}

.main-navigation ul li.menu-item-has-children {
    position: relative;
}

